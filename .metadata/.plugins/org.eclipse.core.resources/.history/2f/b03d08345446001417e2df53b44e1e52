package ru.spbstu.telematics.domrachev.lab02;

/**
 * Двусвязный список.
 */
public class ILinkedList<T> {
	int size = 0;
	private class INode<T>{
		T item;
		INode<T> prev;
		INode<T> next;
		
		INode(T item, INode<T> prev, INode<T> next) {
            this.item = item;
            this.next = next;
            this.prev = prev;
        }
	}
	private INode<T> head;
	private INode<T> tail;
	
	public ILinkedList(){
		head.next = tail.prev;
	}
	
	/**
	 * Добавить элемент в голову списка
	 */
	void addFirst(Object o){
		INode<T> t = new INode<T>((T) o, head, head.next);
		head.next.prev = t;
		head.next = t;
		size++;
	}

	/**
	 * Добавить элемент в хвост списка
	 * 
	 * @param o
	 */
	void addLast(Object o){
		INode<T> t = new INode<T>((T) o, tail.prev, tail);
		tail.prev.next = t;
		tail.prev = t;
		size++;
	}

	/**
	 * Взять первый элемент. Но не удалять из списка!
	 * 
	 * @return
	 */
	Object getFirst(){
		return new Object();
	}

	/**
	 * Взять последний элемент. Но не удалять!
	 * 
	 * @return
	 */
	Object getLast(){
		return new Object();
	}

	/**
	 * Вернуть первый элемент и удалить его из списка.
	 * 
	 * @return
	 */
	Object removeFirst(){
		return new Object();
	}

	/**
	 * Вернуть последний элемент и удалить его из списка.
	 * 
	 * @return
	 */
	Object removeLast(){
		return new Object();
	}
}
